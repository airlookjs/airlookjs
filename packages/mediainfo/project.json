{
    "name": "mediainfo",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "tags": ["type:app"],
	"projectType": "application",
	"targets": {
		"lint": {
		  "executor": "@nx/eslint:lint",
		  "outputs": ["{options.outputFile}"]
		},
		"build": {
			"executor": "@nx/js:tsc",
			"outputs": ["{options.outputPath}"],
			"options": {
			  "outputPath": "dist/mediainfo",
			  "main": "packages/mediainfo/src/index.ts",
			  "tsConfig": "packages/mediainfo/tsconfig.lib.json",
			  "assets": []
			}
		  }
	  }
}