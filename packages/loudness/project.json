{
    "name": "loudness",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "tags": ["type:app"],
    "projectType": "application",
    "sourceRoot": "packages/loudness/src",
    "targets": {
        "lint": {
          "executor": "@nx/eslint:lint",
          "outputs": ["{options.outputFile}"]
        },
        "build": {
          "executor": "@nx/js:tsc",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/loudness",
            "main": "packages/loudness/src/index.ts",
            "tsConfig": "packages/loudness/tsconfig.lib.json",
            "assets": ["packages/loudness/bin/*", "packages/loudness/*.md"]
          }
        },
        "docker": {
          "executor": "@nx-tools/nx-container:build",
          "options": {
          }
        },
        "test": {
          "executor": "@nx/vite:test",
          "outputs": ["{options.reportsDirectory}"],
          "options": {
            "reportsDirectory": "../coverage/libs/util"
          }
        }
      }
}