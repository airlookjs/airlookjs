		"lint": "nx lint",
		"lint-prettier-legacy": "prettier --check . && eslint .",
		"format-legacy": "prettier --write ."
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^3.0.1",
		"@sveltejs/kit": "^2.5.4",
		"@sveltejs/package": "^2.3.0",
		"@sveltejs/vite-plugin-svelte": "^3.1.0",
		"autoprefixer": "^10.4.18",
		"dayjs": "^1.11.10",
		"dequal": "^2.0.3",
		"flowbite": "^2.3.0",
		"flowbite-svelte": "^0.44.24",
		"lodash": "^4.17.21",
		"postcss": "^8.4.37",
