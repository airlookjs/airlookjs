{
	"name": "node-healthcheck",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "packages/node-healthcheck/src",
	"projectType": "library",
	"tags": ["type:util", "scope:shared"],
	"targets": {
		"lint": {
		  "executor": "@nx/eslint:lint",
		  "outputs": ["{options.outputFile}"]
		},
		"build": {
			"executor": "@nx/js:tsc",
			"outputs": ["{options.outputPath}"],
			"options": {
				"outputPath": "dist/node-healthcheck",
				"main": "packages/node-healthcheck/src/index.ts",
				"tsConfig": "packages/node-healthcheck/tsconfig.lib.json",
				"assets": []
			}
		},
		"test": {
			"executor": "@nx/vite:test",
			"outputs": ["{options.reportsDirectory}"],
			"options": {
				"reportsDirectory": "../coverage/libs/util"
			}
		}
	  }
}
